        <updateservers>
                <!-- Note: No spaces or linebreaks allowed between the server tags -->
                <server type="extension" priority="2" name="Zh GoogleMap Package Update Site">http://zhuk.cc/update/pkg_zhgooglemap/extension.xml</server>
        </updateservers>

</extension>

